= form_for [@like.user, @like] do |f|
  -if @like.errors.any?
    #error_explanation
      %h2= "#{pluralize(@like.errors.count, "error")} prohibited this like from being saved:"
      %ul
        - @like.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :related_user_id
    = f.number_field :related_user_id
  .field
    = f.label :is_match
    = f.check_box :is_match
  .actions
    = f.submit 'Save'
